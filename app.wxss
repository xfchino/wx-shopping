/**app.wxss**/
@import "./styles/iconfont.wxss";
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
page{
  /* 主题演色 */
  --theme:#fe3b98;
  --theme-tint:#f32353;
  /* 字体颜色 */
  --text-color-tint:#fc3971;
  --text-color:#fff;
  --text-gray:#b7b7b7
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
